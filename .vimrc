set nobackup
set fenc=utf-8
set number
set cursorline
set smartindent
set showmatch
set wildmenu
set wildmode=list:longest
nnoremap j gj
nnoremap k gk
set expandtab
set tabstop=2
set shiftwidth=2
set wrapscan
set hlsearch
set virtualedit=block
set backspace=indent,eol,start
set ambiwidth=double
set laststatus=2
syntax on
set colorcolumn=80
inoremap <S-tab> <BS><BS>
